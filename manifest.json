{
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16-disconnected.png",
      "32": "icons/32-disconnected.png"
    }
  },
  "description": "Powiadamia o strumieniach Jadisco.pl.",
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png"
  },
  "manifest_version": 3,
  "minimum_chrome_version": "116",
  "name": "Jadisco.pl",
  "permissions": [
    "notifications",
    "offscreen",
    "storage",
    "background"
  ],
  "version": "2.1.0",
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
